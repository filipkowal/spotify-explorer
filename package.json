{
  "author": "Spotify",
  "name": "web-api-auth-examples",
  "description": "Basic examples of the Spotify authorization flows through OAuth 2",
  "version": "0.0.2",
  "engines": {
    "node": "14.5.x"
  },
  "dependencies": {
    "concurrently": "^5.3.0",
    "cookie-parser": "1.3.2",
    "cors": "^2.8.4",
    "express": "~4.16.0",
    "querystring": "~0.2.0",
    "request": "~2.83.0",
    "teenyicons": "^0.4.0"
  },
  "scripts": {
    "start": "node authorization_code/app.js",
    "client": "cd client && npm start",
    "dev": "concurrently -n 'server,client' -c 'blue,green' \"nodemon authorization_code/app.js\" \"npm run client\"",
    "install-client": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run install-client && npm run build"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
